{
  "name": "@chrislittle/storybook",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "STORYBOOK_THEME=graphite storybook dev -p 6006",
    "dev:graphite": "STORYBOOK_THEME=graphite storybook dev -p 6006",
    "dev:newspaper": "STORYBOOK_THEME=newspaper storybook dev -p 6007",
    "dev:business": "STORYBOOK_THEME=business storybook dev -p 6008",
    "build": "storybook build",
    "build:graphite": "STORYBOOK_THEME=graphite storybook build -o storybook-static-graphite",
    "build:newspaper": "STORYBOOK_THEME=newspaper storybook build -o storybook-static-newspaper",
    "build:business": "STORYBOOK_THEME=business storybook build -o storybook-static-business",
    "clean": "rm -rf storybook-static storybook-static-*"
  },
  "dependencies": {
    "@chrislittle/theme-core": "workspace:*",
    "@chrislittle/theme-graphite": "workspace:*",
    "@chrislittle/theme-newspaper": "workspace:*",
    "@chrislittle/theme-business": "workspace:*",
    "@chrislittle/ui-primitives": "workspace:*",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.4.0",
    "@storybook/addon-interactions": "^8.4.0",
    "@storybook/addon-links": "^8.4.0",
    "@storybook/addon-themes": "^8.4.0",
    "@storybook/blocks": "^8.4.0",
    "@storybook/react": "^8.4.0",
    "@storybook/react-vite": "^8.4.0",
    "@storybook/test": "^8.4.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "storybook": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
